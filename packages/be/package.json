{
  "name": "back",
  "version": "1.0.50",
  "scripts": {
    "build": "bun build --target=bun ./src/index.ts --outfile=./dist/server.js",
    "build:others": "npm run build",
    "build:render": "npm run install && npm run build",
    "start": "bun dist/server.js",
    "install": "bun install",
    "dev": "bun --watch src/index.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^0.7.1",
    "@elysiajs/html": "^0.7.3",
    "@libsql/kysely-libsql": "^0.3.0",
    "elysia": "latest",
    "kysely": "^0.26.3"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "module": "dist/server.js"
}